
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>elanora</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="/Content/css/kube.min.css" />
    <link rel="stylesheet" href="/Content/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/Content/css/custom.min.css" />
    <link href="http://fonts.googleapis.com/css?family=Lato:400,700" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet" type="text/css">
</head>
<body>
    <!-- Navigation -->
    @Html.Partial("_Header")
    <!-- Introduction -->
    <div class="intro">
        <div class="container">
            <div class="units-row">
                <div class="unit-10">
                    <img class="img-intro" src="/Content/img/logom.jpg" alt="">
                </div>
                <div class="unit-90">
                    <p class="p-intro">Merhaba, ben elanora dünyama hoşgeldiniz.</p>
                </div>
            </div>
        </div>
    </div>

    <!-- Content -->

    @RenderBody()<br /><br /><br />

    <!--footer-->
    @Html.Partial("_Footer")
    <!-- Javascript -->
    <script src="/Content/js/jquery.min.js"></script>
    <script src="/Content/js/kube.min.js"></script>
</body>
</html>